{
  "installCommand": "pnpm install",
  "buildCommand": "pnpm run build",
  "build": {
    "env": {
      "NODE_VERSION": "20",
      "PNPM_HOME": "/vercel/.cache/pnpm",
      "COREPACK_ENABLE_DOWNLOAD_PROMPT": "0",
      "NPM_CONFIG_FUND": "false",
      "NPM_CONFIG_OPTIONAL": "true",
      "npm_config_optional": "true",
      "TAILWIND_DISABLE_OXIDE": "1"
    }
  }
}
