# Camber Cloud Configuration
# Get these from https://camber.ai dashboard after deployment
CAMBER_JOB_URL=https://api.camber.ai/v1/jobs/YOUR_JOB_ID/run
CAMBER_API_KEY=your_camber_api_key_here

# Appwrite Configuration
# Get these from https://cloud.appwrite.io console
APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
APPWRITE_PROJECT_ID=your_appwrite_project_id
APPWRITE_API_KEY=your_appwrite_api_key
APPWRITE_BUCKET_ID=chat-videos

# Optional: Custom Font Paths
# CHAT_FONT=/usr/share/fonts/truetype/dejavu/DejaVuSans.ttf
# CHAT_FONT_BOLD=/usr/share/fonts/truetype/dejavu/DejaVuSans-Bold.ttf

# Development Settings
# NODE_ENV=development
# LOG_LEVEL=debug
